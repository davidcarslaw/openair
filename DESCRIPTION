Package: openair
Type: Package
Title: Tools for the Analysis of Air Pollution Data
Version: 1.1-1
Date: 2015-01-13
Authors@R: c(person("David", "Carslaw", role = c("aut", "cre"), email =
    "david.carslaw@kcl.ac.uk"), person("Karl", "Ropkins", role =
    "aut", email = "K.Ropkins@its.leeds.ac.uk"))
ByteCompile: true
Depends:
    R (>= 2.15.1),
    maps,
Imports:
    utils,
